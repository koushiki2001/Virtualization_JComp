  <div class="container">
  <div class="row">
 
<% items.forEach(function(item) { %>
  
    
    <div class="col-lg-4">
  <div class="card" style="width: 15rem; margin:1rem;">
    <div class="card-body">
      <h5 class="card-title"><%= item.Title %></h5>
      <h6 class="card-subtitle mb-2 text-muted">Author: <%= item.Author %></h6>
      <p class="card-text">Publisher: <%= item.Publisher %></p>
      <p class="card-text">Pages: <%= item.Pages %></p>
      <a href="http://localhost:3000/book/<%= item.ID %>" class="card-link"><button class = "btn btn-info">View</button></a>
      <a href="http://localhost:3000/bookdelete/<%= item.ID %>" class="card-link"><button class = "btn btn-warning">Delete</button></a>
